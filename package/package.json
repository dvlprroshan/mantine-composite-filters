{
  "name": "mantine-composite-filters",
  "version": "0.4.0",
  "description": "A powerful composite filters component for Mantine",
  "homepage": "https://dvlprroshan.github.io/mantine-composite-filters",
  "packageManager": "yarn@4.0.1",
  "license": "MIT",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "keywords": [
    "mantine",
    "filters",
    "composite-filters",
    "react",
    "ui"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css"
  },
  "repository": "https://github.com/dvlprroshan/mantine-composite-filters.git",
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/dates": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "@mantine/notifications": ">=7.0.0",
    "@tabler/icons-react": ">=2.0.0",
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "bugs": "https://github.com/dvlprroshan/mantine-composite-filters/issues"
}
